{
  "name": "beatonma.org",
  "type": "commonjs",
  "version": "1.0.0",
  "description": "beatonma.org",
  "scripts": {
    "build": "gulp production",
    "buildTest": "gulp test",
    "watch": "gulp dev",
    "watchWebapps": "gulp devWithWebapps",
    "jest": "jest"
  },
  "typings": "./types",
  "keywords": [
    "beatonma"
  ],
  "author": "Michael Beaton",
  "license": "ISC",
  "browserslist": [
    "last 2 versions",
    "not dead",
    "> 0.2%"
  ],
  "prettier": {
    "arrowParens": "avoid",
    "tabWidth": 4,
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ],
    "trailingComma": "all",
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "tabWidth": 2
        }
      }
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard-scss",
    "rules": {
      "custom-property-empty-line-before": null,
      "color-hex-length": "long",
      "color-hex-case": "lower",
      "selector-id-pattern": null
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@swc/core": "^1.3.96",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/browser-sync": "^2.27.4",
    "@types/glob": "^8.1.0",
    "@types/gulp": "^4.0.17",
    "@types/gulp-autoprefixer": "^0.0.37",
    "@types/gulp-if": "^3.0.4",
    "@types/gulp-postcss": "^8.0.6",
    "@types/gulp-rename": "^2.0.5",
    "@types/gulp-sass": "^5.0.2",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.10",
    "@types/vinyl-named": "^1.1.4",
    "@types/webpack-stream": "^3.2.15",
    "autoprefixer": "^10.4.16",
    "browser-sync": "^2.29.3",
    "css-loader": "^6.8.1",
    "cssnano": "^6.0.1",
    "cypress": "13.5.1",
    "del": "^6.1.1",
    "gulp": "^4.0.2",
    "gulp-if": "^3.0.0",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.4",
    "gulp-sass": "^5.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.19.0",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "swc-loader": "^0.2.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vinyl-named": "^1.1.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-stream": "^7.0.0"
  }
}
